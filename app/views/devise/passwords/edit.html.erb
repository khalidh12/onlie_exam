</script>
  <div class="actions">
    <%= f.submit "Change my password", :class => "btn btn-primary" %>
  </div>
<% end %>

<%= link_to "Log in", new_session_path(resource_name), :class => "btn btn-success" %>
<%= link_to "Sign up", new_registration_path(resource_name), :class => "btn btn-success" %><br />
