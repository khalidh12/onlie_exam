<h2><b>Log in</b></h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :placeholder => "sbtetworld@gmail.com", :class => "form-control focus-field", :required => true%>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", :placeholder => "*********", :class => "form-control focus-field", :required => true %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
